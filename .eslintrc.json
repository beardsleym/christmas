{
  "extends": [
    // "airbnb-base",
    // "airbnb",
    // "airbnb-typescript",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "no-console": "error",
    "no-unused-vars": "warn",
    "import/order": "warn",
    "no-use-before-define": "warn",
    "no-duplicate-imports": "warn",
    "no-shadow": "error",
    "arrow-body-style": "error"
  },
  "reportUnusedDisableDirectives": true
}
